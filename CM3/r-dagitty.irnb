      "cell_type": "code",
      "source": [
        "#generate a couple of DAGs and plot them\n",
        "\n",
        "G = dagitty('dag{\n",
        "Z1 [pos=\"-2,-1.5\"]\n",
        "X1 [pos=\"-2,0\"]\n",
        "Z2 [pos=\"1.5,-1.5\"]\n",
        "X3 [pos=\"1.5, 0\"]\n",
        "Y [outcome,pos=\"1.5,1.5\"]\n",
        "D [exposure,pos=\"-2,1.5\"]\n",
        "M [mediator, pos=\"0,1.5\"]\n",
        "X2 [pos=\"0,0\"]\n",
        "Z1 -> X1\n",
        "X1 -> D\n",
        "Z1 -> X2\n",
        "Z2 -> X3\n",
        "X3 -> Y\n",
        "Z2 -> X2\n",
        "D -> Y\n",
        "X2 -> Y\n",
        "X2 -> D\n",
        "M->Y\n",
        "D->M\n",
        "}')\n",
        "\n",
        "\n",
        "ggdag(G)+  theme_dag()"
      ],
